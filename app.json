{
  "expo": {
    "name": "ecomercado-app",
    "slug": "patigayonmobileapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/ecomercado-icon.png",
    "notification": {
      "icon": "./assets/ecomercado-icon.png",
      "color": "#ffffff", 
      "androidMode": "default",
      "androidCollapsedTitle": "#{unread_notifications} new interactions"
    },
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/background.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.uc.ecomercado",
      "adaptiveIcon": {
        "foregroundImage": "./assets/ecomercado-icon.png",
        "backgroundColor": "#ffffff"
      },
      "googleServicesFile": "./google-services.json",
      "useNextNotificationsApi": true,
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION",
        "POST_NOTIFICATIONS",
        "NOTIFICATIONS",
        "VIBRATE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "9c6726c2-1c49-48e9-8467-40c38c0776ee"
      }
    },
    "owner": "jasonpatigayon25"
  }
}
